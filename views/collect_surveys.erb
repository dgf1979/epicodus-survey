<h1 class="page-header text-center">Available Surveys</h1>

<div class="row">
  <div class="col-sm-8 col-sm-offset-2">

    <div class="push-down">
      <h4>Surveys:</h4>

      <table class="table">
      <% @surveys.each() do |survey| %>
        <tr>
          <td class="primary">
            <a href="/collect/surveys/<%= survey.id %>"><%= survey.title %></a>
          </td>
        </tr>
      <% end %>
    </table>
    </div>


  </div>
</div>

